{
    "name": "slidize-cloud/php-sdk",
    "version": "24.9.0",
    "description": "Slidize.Cloud Web API for managing PowerPoint presentations.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://slidize.cloud",
    "license": "MIT",
    "authors": [
        {
            "name": "Slidize.Cloud Team",
            "homepage": "https://slidize.cloud"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Slidize\\Cloud\\Sdk\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Slidize\\Cloud\\Sdk\\Test\\" : "test/" }
    }
}
